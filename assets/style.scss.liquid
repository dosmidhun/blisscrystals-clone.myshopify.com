        display: inline-block;
      }

      &:hover {
        a {
          color: $link_color_hover;
          border-bottom: 1px solid lighten($link_color_hover, 20%);
          span{
            color: $link_color_hover;
            
          }
        }
      }
      a{
        cursor: pointer;
        line-height: 15px;
      }

      &.active{
        color: $link_color_hover;
        span {
          color: $link_color_hover; 
          border-bottom: 1px solid lighten($link_color_hover, 20%);
          line-height: 15px;
          display: inline-block;
        }
      }

      &:first-of-type, &:last-of-type {
        position: relative;
        &:before {
          font-family: FontAwesome;
          position: absolute;
          top: 0px;
          font-size: 16px;
          color: $page_title_color;
        }
        &:hover {
          &:before {
            color: $link_color_hover; 
          }
        }
      }
      &:last-of-type {
        margin-right: -3px;
        padding-right: 15px;
        &:before {
          content: "\f105";
          right: 0;
        }
      }

      &:first-of-type {
        margin-left: -2px;
        padding-left: 15px;
        &:before {
          content: "\f104";
          left: 0;
        }
      }

      &.disabled {
        &:hover {
          &:before {
            color: $page_title_color; 
          }
        }
      }
    }
  }
}

