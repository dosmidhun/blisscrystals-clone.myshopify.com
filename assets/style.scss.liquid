  .translate-overlay {
    position: fixed;
    background-color: rgba(0,0,0,.2);
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    cursor: pointer;
    display: none;
    z-index: 3;
  }

  &.translate {
    position: fixed;
    z-index: 2;
    top: 0 !important;
    left: 0;
    right: 0;
    bottom: 0;
    overflow-y: hidden;
    cursor: pointer;

    .translate-overlay {
