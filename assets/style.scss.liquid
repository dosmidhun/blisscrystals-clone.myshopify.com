    .btn{
      padding: 7px 25px;
      font-size: 12px;
      text-transform: capitalize;
      font-weight: 500;
      letter-spacing: 0;
    }
  }


}

.ajax-error-modal {
  left: 0;
  top: 50% !important;
  overflow: hidden;
  padding: 10px 20px;
  width: 100%;
  text-align: center;
