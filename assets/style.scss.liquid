.ajax-success-modal {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(25,25,25,0.75);
  .overlay {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
  }
  .content {
    position: fixed;
    top: 50%;
    left: 50%;
    @include transform(translate(-50%,-50%));
    z-index: 10;
    max-width: 588px;
    background: #fff;
    overflow: visible;
    width: 90%;
    &:after {
      display: block;
      content: "";
      visibility: hidden;
      clear: both;
    }
  }

  .ajax-left {
    width: 143px;
    float: left;
    img {
      max-width: 143px;
      max-height: 170px;
    }
  }

  .ajax-right {
    float: right;
    @include calc(width, '100% - 143px');
    padding: 30px 40px 30px 30px;
  }

  .success-message {
    color: #787878;
    font-weight: 400;
    display: block;
    line-height: 22px;
    margin-top: 2px;
    margin-bottom: 3px;
  }

  .ajax-product-title {
    color: $color_title_pr;
    font-weight: 500;
