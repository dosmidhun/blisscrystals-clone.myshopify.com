    display: inline-block;
    > span {
      background-image: linear-gradient(#fff 95%, $color_title_pr 5%);
      background-repeat: repeat-x;
      background-position-y: 0px;
      line-height: 18px;
    }
  }

  .ajax-product-title,
  .success-message {
    font-size: $font_size;
    font-family: $font_1;
  }

  .actions{
    padding-top:20px;
    clear: both;
    .continue-shopping{
      @include button_1();
      min-width: 188px;
    }
    .btn-go-to-cart{
      @include button_1();
      min-width: 131px;
